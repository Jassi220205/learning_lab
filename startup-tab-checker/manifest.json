{
  "manifest_version": 3,
  "name": "Startup Tab Checker",
  "version": "1.1",
  "description": "Opens your chosen websites at startup.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
